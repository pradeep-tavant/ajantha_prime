<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <div class="panel panel-default">
        <div class="panel-heading"><h3>New Member</h3></div>
        <div class="panel-body panel30">
          <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: {class: "form-horizontal"}) do |f| %>
            <%= devise_error_messages! %>

            <%= render 'members/form_fields', f: f, member: resource %>

            <div class="col-sm-12 flex-buttons">
              <%= link_to 'Cancel', new_member_session_path, class: "btn btn-default" %>
              <%= f.submit "Sign Up", class: "mleft10 btn btn-primary" %>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>